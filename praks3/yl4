#/bin/bash
# busside arvu arvutamine

echo -n "Sisesta reisijate arv: "
read reisijad

echo -n "Kohtade arv 체hes bussis: "
read kohad_bussis

bussid=$(( reisijad / kohad_bussis ))

echo "T채is busside arv on $bussid"

maha_jaanud=$(( reisijad % kohad_bussis ))

echo "Maha on j채채nud $maha_jaanud inimest"
